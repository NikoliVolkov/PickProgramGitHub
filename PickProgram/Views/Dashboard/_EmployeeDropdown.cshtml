@model List<SelectListItem>

    <select asp-items="@Model" class="form-control">
        <option value="">-- Assign Employee --</option>
    </select><div class="btn-group btn-group-sm"><button onclick="assignEmp($(this))" title="Confirm changes" type="button" class="btn btn-default"><span class="glyphicon glyphicon-ok-circle text-success" aria-hidden="true"></span></button><button onclick="restoreAddEmployee($(this))" title="Cancel" type="button" class="btn btn-default"><span class="glyphicon glyphicon-remove-circle text-danger" aria-hidden="true"></span></button></div>
